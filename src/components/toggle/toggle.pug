mixin toggle(options, val)
  if (typeof(options) !== null && typeof(val) !== null)
    div.toggle
      label(class=['toggle__label'] for= val.toggleId)
        input(type='checkbox' class= ['toggle__input'] value= val.toggleValue id= val.toggleId name= val.toggleName checked=val.checked)
        span(class= ['toggle__slider', 'toggle__slider--rounded'])
        span(class='toggle__title') #{val.toggleTitle}
